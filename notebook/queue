#include <iostream>
#include <queue>

using namespace std;


int main()

{
  int i;
  queue <int> myQ;

  cout<< "现在 queue 是否 empty? "<< myQ.empty() << endl; 

  for( i =0; i<10 ; i++)
  {
     myQ.push(i);
  }

  for( i=0; i<myQ.size(); i++)
  {
	  cout<<"myQ.size():"<<myQ.size()<<endl;
//	printf("myQ.size():%d\n",myQ.size());
	cout << myQ.front()<<endl;
	myQ.pop();
  }
  return 0;
}
